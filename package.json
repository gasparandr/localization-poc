{
  "name": "JSON-localization",
  "version": "1.0.0",
  "description": "Localization for application JSON configs",
  "main": "index.js",
  "scripts": {
    "translate": "node src/index.js",
    "start": "electron .",
    "dist": "build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gasparandr/localization-poc.git"
  },
  "author": "Andrei Gaspar",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gasparandr/localization-poc/issues"
  },
  "homepage": "https://github.com/gasparandr/localization-poc#readme",
  "dependencies": {
    "progressbar.js": "^1.0.1",
    "selenium-webdriver": "^4.0.0-alpha.1"
  },
  "devDependencies": {
    "electron": "^4.0.5",
    "electron-builder": "^20.38.5"
  },
  "build": {
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "deleteAppDataOnUninstall": true,
      "createDesktopShortcut": true,
      "installerIcon": "src/client/style/img/icons/icon.ico",
      "uninstallerIcon": "src/client/style/img/icons/icon.ico",
      "uninstallDisplayName": "Uninstall JSON Localization"
    },
    "appId": "com.localization.poc",
    "win": {
      "target": "nsis",
      "icon": "src/client/style/img/icons/icon.ico",
      "publisherName": "Andrei"
    },
    "productName": "JSON Localization",
    "buildVersion": "1.0.0",
    "copyright": "Andrei 2019",
    "extraFiles": [
      "chromedriver.exe"
    ]
  }
}
